each article in articles
  div.article-verticle-item(navId="#{article.navId}" articleId="#{article._id}" categoryId="#{params.categoryId}")
    img.icon(src="#{article.img}", alt="")
    div.content
      h2.title
        a(href="/article/detail/view/#{article._id}/1") #{article.title}
      p.time #{article.createdTime}  
      p.description #{article.description}