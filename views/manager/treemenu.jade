ul#panelWrapper
    - var navs = data.navs;
    - var navStr = JSON.stringify(navs);
    li(panel-item-type=0 navs="#{navStr}") 名称
      ul
       each nav,navIndex in navs
         - var str = JSON.stringify(nav.categories);
         li(categories="#{str}" panel-item-type=1 navId="#{nav._id}").panel-item #{nav.name}
           ul
             each category in nav.categories
               li.category-item(navId="#{nav._id}" categoryId="#{category._id}" panel-item-type=2) #{category.name}   
