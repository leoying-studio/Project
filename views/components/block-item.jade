- var articles = data.articles;
each article in articles
  div.article-block
   img(src="/images/1.jpg").article-block-img 
   h2.title #{article.title}
   div.article-info
    span.creation-type 原创
    span.article-footer-between  
     i.fa.fa-commenting-o
     i 400
    span.article-footer-between 
      i.fa.fa-eye 
      i 400  
   div.description #{article.description}